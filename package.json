{
  "name": "advanced-react",
  "version": "1.0.0",
  "description": "Advanced React Project",
  "main": "lib/server.js",
  "author": "Edgar Pino",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "ejs": "^2.5.7",
    "eslint": "^4.11.0",
    "express": "^4.16.2",
    "lodash.debounce": "^4.0.8",
    "lodash.pickby": "^4.6.0",
    "pm2": "^2.7.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "webpack": "^3.8.1"
  },
  "babel": {
    "presets": [
      "react",
      ["env", { "targets": { "node": "current" }}],
      "stage-2"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "scripts": {
    "dev": "NODE_PATH=./lib pm2 start lib/server.js --watch --interpreter babel-node",
    "webpack": "webpack -wd",
    "test": "jest --watch",
    "build:webpack": "webpack -p",
    "build:node": "babel lib -d build --copy-files",
    "start:prod": "NODE_ENV=production NODE_PATH=./build pm2 start build/server.js -i max --name prod-node-app"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "react-test-renderer": "^16.1.1"
  }
}
